{
  "name": "portfolio",
  "version": "4.0.0",
  "type": "module",
  "description": "Personal portfolio using React",
  "repository": {
    "type": "git",
    "url": "https://github.com/TzolkinB/portfolio-react"
  },
  "scripts": {
    "start": "webpack serve --config build-utils/webpack.config.cjs --env env=dev",
    "build": "webpack --config build-utils/webpack.config.cjs --env env=prod",
    "build:analyze": "npm run build",
    "deploy": "aws s3 sync dist/ s3://www.kimbell.me --delete && aws cloudfront create-invalidation --distribution-id E2UIRXEU09EPZZ --paths \"/*\"",
    "prod": "npm run build && npm run deploy",
    "lint": "eslint .",
    "test": "cypress open --e2e",
    "cy:test": "start-server-and-test start http://localhost:4280 test",
    "cy:run": "start-server-and-test start http://localhost:4280 'cypress run'",
    "cy:headless": "start-server-and-test start http://localhost:4280 'cypress run --browser chrome --headless'",
    "lint:js": "eslint . --max-warnings=0",
    "lint:yaml": "yamllint .github/**/*.yml",
    "lint:husky": "npm run lint:js && npm run lint:yaml"
  },
  "author": "Kimberly Bell",
  "license": "UNLICENSED",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.2.0",
    "mdb-react-ui-kit": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.0",
    "@cypress/webpack-preprocessor": "^6.0.1",
    "@eslint/js": "^9.39.2",
    "@testing-library/cypress": "~10.1.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^7.1.3",
    "cypress": "^15.10.0",
    "dotenv-webpack": "^8.1.1",
    "eslint": "~9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^5.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.1.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "~5.0.0",
    "typescript-eslint": "^8.55.0",
    "webpack": "^5.90.0",
    "webpack-bundle-analyzer": "^5.2.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  }
}
